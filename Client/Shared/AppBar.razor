
<MudAppBar Color="Color.Primary" Elevation="3">
    <MudIconButton Icon="@Icons.Material.Filled.Menu" Color="Color.Inherit" Edge="Edge.Start" OnClick="@MenuIconOnClick" />
    <MudSpacer />
    <MudIconButton Icon="@Icons.Filled.ColorLens" Color="Color.Default" OnClick="@ToggleTheme" />
</MudAppBar>

@code {


    [Parameter]
    public EventCallback ToggleTheme { get; set; }

    [Parameter]
    public EventCallback MenuIconOnClick { get; set; }

}
